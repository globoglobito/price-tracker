          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  CASE \n    WHEN seller_location IS NULL OR seller_location = '' THEN 'Unknown'\n    WHEN LOWER(seller_location) LIKE '%usa%' OR LOWER(seller_location) LIKE '%united states%' THEN 'United States'\n    WHEN LOWER(seller_location) LIKE '%japan%' THEN 'Japan'\n    WHEN LOWER(seller_location) LIKE '%china%' THEN 'China'\n    WHEN LOWER(seller_location) LIKE '%netherlands%' OR LOWER(seller_location) LIKE '%holland%' THEN 'Netherlands'\n    ELSE 'Other'\n  END as metric,\n  COUNT(*) as value\nFROM price_tracker.listings\nWHERE scraped_at >= NOW() - INTERVAL '24 hours'\nGROUP BY country\nORDER BY value DESC",
          "refId": "A"
        }
      ],
      "title": "Seller Locations by Country",
